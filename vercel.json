{
  "framework": "nextjs",
  "functions": {
    "app/api/auth/[...nextauth]/route.ts": {
      "maxDuration": 30
    },
    "app/api/blog/posts/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/upload/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/admin/invoices/[id]/pdf/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "env": {
    "NEXTAUTH_URL": "https://shopify-headless-store-sigma.vercel.app",
    "NEXTAUTH_URL_INTERNAL": "https://shopify-headless-store-sigma.vercel.app"
  },
  "headers": [
    {
      "source": "/api/auth/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    },
    {
      "source": "/api/blog/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}
